{
  "routes": [
    {
      "version": "1.0",
      "prefix": "list",
      "name": "test",
      "description": "A test route",
      "dispatcher": {
        "class": "Dispatch"
      },
      "request": {
        "method": "get",
        "headers": {
          "schema": {
            "type": "object",
            "properties": {
              "content-type": {
                "type": "string",
                "pattern": "^application/json$"
              }
            }
          }
        },
        "parameters": {
          "schema": {
            "type": "object",
            "properties":{
              "age": {
                "type": "integer",
                "description": "Age of the user",

                "LDL": {}
              },
              "name": {
                "type": "string",
                "description": "Name of the student",
                "minLength": 5,
                "LDL": {
                  "converter": {
                    "class": "NameTransformer"
                  },
                  "immutable": true
                }
              }
            },
            "required": ["name", "age"]
          }
        }
      },
      "response": {
        "contentType": "application/json",
        "cache": {
          "config": {
            "purgeable": true,
            "enabled": true,
            "expiresAt": "10 second"
          },
          "adapter": {
            "class": "Symfony\\Component\\Cache\\Adapter\\FilesystemAdapter",
            "arguments":{
              "namespace": "",
              "defaultLifeTime": 0
            }
          }
        }
      }
    }
  ]
}