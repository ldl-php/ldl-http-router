{
  "routes": [
    {
      "name": "test",
      "version": "1.0",
      "url": {
        "prefix": "list/{urlName}"
      },
      "request": {
        "method": "get"
      },
      "description": "A test route",
      "middleware": {
        "dispatchers": [
          "dispatcher",
          "dispatcher2"
        ]
      },
      "response": {
        "exception": {
          "handlers": [
            "test.exception.handler"
          ]
        }
      },
      "customConfig": {
        "test": "1"
      }
    },
    {
      "version": "1.0",
      "url": {
        "prefix": "exception"
      },
      "request": {
        "method": "get"
      },
      "name": "exception",
      "description": "Exception test",
      "middleware": {
        "dispatchers": "dispatcher2",
        "preDispatch": ["predispatch"]
      },
      "response": {
        "exception": {
          "handlers": [
            "test.exception.handler"
          ]
        }
      }
    }
  ]
}