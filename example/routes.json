{
  "routes": [
    {
      "name": "test",
      "version": "1.0",
      "url": {
        "prefix": "list/{urlName}"
      },
      "request": {
        "method": "get"
      },
      "description": "A test route",
      "middleware": {
        "dispatchers": {
          "name": "main dispatchers",
          "list": [
            {"groupA": [
              {"groupB": [
                {"groupC": ["dispatcher"]},
                "dispatcher3"
              ]}
            ]},
            "dispatcher2"
          ]
        },
        "postDispatch": {
          "name": "Post dispatchers",
          "list": [
            "post.dispatch"
          ]
        }
      },
      "response": {
        "exception": {
          "handlers": [
            "test.exception.handler"
          ]
        },
        "parser": {
          "name": "ldl.response.parser.json",
          "options": {
            "options": "JSON_PRETTY_PRINT | JSON_THROW_ON_ERROR",
            "content-type": "application/json"
          }
        }
      },
      "customConfig": {
        "test": "1"
      }
    },
    {
      "version": "1.0",
      "url": {
        "prefix": "exception"
      },
      "request": {
        "method": "get"
      },
      "name": "exception",
      "description": "Exception test",
      "middleware": {
        "dispatchers": "dispatcher2",
        "preDispatch": ["predispatch"]
      },
      "response": {
        "exception": {
          "handlers": [
            "test.exception.handler"
          ]
        }
      }
    }
  ]
}