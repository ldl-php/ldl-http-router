{
  "routes": [
    {
      "version": "1.0",
      "url": {
        "prefix": "handler/{urlName}"
      },
      "request": {
        "method": "get"
      },
      "name": "testRoute",
      "description": "A test route",
      "dispatcher": {
        "class": "Dispatch"
      }
    },
    {
      "version": "2.0",
      "url": {
        "prefix": "handler/{urlName}"
      },
      "request": {
        "method": "get"
      },
      "name": "testLocalExceptionHandlerRoute",
      "description": "This route use a local exception handler",
      "dispatcher": {
        "class": "Dispatch"
      },
      "handlers": {
        "exceptions": [
          {
            "class": "testingExceptionHandler",
            "active": true,
            "priority": 1
          }
        ]
      }
    }
  ]
}